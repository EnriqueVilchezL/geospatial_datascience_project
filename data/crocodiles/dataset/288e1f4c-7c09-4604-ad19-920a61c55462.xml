            <project >
                <title>Digitizing the major vertebrate tissue collection in the Middle East</title>
                <personnel>
                    <individualName>
                    <givenName>Shai</givenName>
                    <surName>Meiri</surName>
                    </individualName>
                    <role>PRINCIPAL_INVESTIGATOR</role>
