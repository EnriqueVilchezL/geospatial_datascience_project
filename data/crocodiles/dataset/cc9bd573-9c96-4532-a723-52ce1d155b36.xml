            <project >
                <title>Africa Biodiversity Challenge</title>
                <personnel>
                    <individualName>
                    <givenName>Yvette</givenName>
                    <surName>Umurungi</surName>
                    </individualName>
                    <role>POINT_OF_CONTACT</role>
